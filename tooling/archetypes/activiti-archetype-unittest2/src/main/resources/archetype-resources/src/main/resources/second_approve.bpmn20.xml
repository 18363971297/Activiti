  <process id="second_approve" name="二级审批流程" isExecutable="true">
    <startEvent id="startEvent" name="开始"></startEvent>
    <userTask id="submit" name="请假审批填写提交">
      <extensionElements>
        <activiti:formProperty id="name" name="申请人" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="message" name="申请原因" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="submitTime" name="申请时间" type="date" datePattern="yyyy-MM-dd" required="true"></activiti:formProperty>
        <activiti:formProperty id="submitFlag" name="是否提交" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startEvent" targetRef="submit"></sequenceFlow>
    <exclusiveGateway id="isSubmit" name="是否提交"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="submit" targetRef="isSubmit"></sequenceFlow>
    <userTask id="approve" name="主管审批">
      <extensionElements>
        <activiti:formProperty id="approveFlag" name="是否审批通过" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="approveMassage" name="审批备注" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="isSubmit" targetRef="approve">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${submitFlag == "Y" || submitFlag == "y"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="isApprove" name="是否审批通过"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="approve" targetRef="isApprove"></sequenceFlow>
    <userTask id="pass" name="hr批准">
      <extensionElements>
        <activiti:formProperty id="passFlag" name="是否批准" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="passMassage" name="批准备注" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow5" sourceRef="isApprove" targetRef="pass">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approveFlag == "Y" || approveFlag == "y"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="isPass" name="是否通过"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="pass" targetRef="isPass"></sequenceFlow>
    <endEvent id="endEvent" name="结束"></endEvent>
    <sequenceFlow id="flow7" sourceRef="isPass" targetRef="endEvent">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${passFlag == "Y" || passFlag == "y"}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endCannelEvnet" name="取消"></endEvent>
    <sequenceFlow id="flow8" sourceRef="isSubmit" targetRef="endCannelEvnet">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${submitFlag == "N" || submitFlag == "n"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="isApprove" targetRef="submit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approveFlag == "N" || approveFlag == "n"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="isPass" targetRef="submit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${passFlag == "N" || passFlag == "n"}]]></conditionExpression>
    </sequenceFlow>
  </process>
